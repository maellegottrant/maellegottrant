from math import sqrt

a=int(input("a:"))
b=int(input("b:"))
c=int(input("c:"))

delta=b**2-4*a*c

if a==0:
  print("ce n'est pas une équation du second degré")

if delta<0:
  print("pas de solution")
  
if delta==0 and a!=0:
  x0=(-b)/2*a
  print("unique solution:", x0)
  
if delta>0 and a!=0:
  x1=((-b)-sqrt(delta))/(2*a)
  x2=((-b)+sqrt(delta))/(2*a)
  print("deux solutions distinctes:", "solution 1:", x1, "solution 2:", x2)
